@model IEnumerable<CarritoDeCompras.Models.StockItem>

@{
    ViewData["Title"] = "StockItem";
}

<h1>Productos</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Producto.Nombre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Cantidad)
            </th>


        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Producto.Nombre)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Cantidad)
                </td>
            </tr>
        }
    </tbody>
</table>
<div>
    <a asp-action="Index" asp-route-id=@ViewData["ProductoId"]>Volver</a>
</div>